// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Asset {
  id Int @default(autoincrement()) @unique
  title String
  income Int
  outcome Int
  price Int
  dept Int
}

